{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "server/bin/www.js",
  "scripts": {
    "build": "babel server -d dist",
    "clean": "rimraf dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "node -r dotenv/config -r @babel/register server/bin/www.js",
    "db:create": "sequelize db:create --env",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run build && npm run migrate",
    "make:migration": "sequelize migration:create --name",
    "make:model": "sequelize model:create --name",
    "make:seed": "sequelize seed:create --name",
    "migrate": "sequelize db:migrate",
    "migrate:refresh": "npm run migrate:reset && npm run migrate",
    "migrate:reset": "sequelize db:migrate:undo:all",
    "migrate:undo": "npm run sequelize -- db:migrate:undo",
    "mocha": "mocha",
    "mocha:watch": "npm run mocha -- -w",
    "prebuild": "npm run clean",
    "seed": "sequelize db:seed:all --seeders-path database/seeders",
    "seed:undo": "sequelize db:seed:undo:all",
    "sequelize": "sequelize",
    "start": "node -r dotenv/config dist/bin/www.js",
    "start:dev": "nodemon --exec npm run dev",
    "seed:test": "cross-env NODE_ENV=test sequelize db:seed:all --seeders-path database/seeders",
    "seed:undo:test": "cross-env NODE_ENV=test sequelize db:seed:undo:all",
    "migrate:test": "cross-env NODE_ENV=test sequelize db:migrate",
    "migrate:reset:test": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "pretest": "cross-env NODE_ENV=test  npm run migrate:refresh && npm run seed:test",
    "test": "nyc --reporter=html --reporter=text --reporter=lcov mocha"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@sendgrid/mail": "^6.4.0",
    "app-root-path": "^2.1.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "chai-passport-strategy": "^1.0.1",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "email-templates": "^5.0.4",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.1",
    "faker": "^4.1.0",
    "http-errors": "^1.7.2",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.11",
    "maildev": "^1.0.0",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "mongoose": "^5.4.16",
    "mongoose-unique-validator": "^2.0.2",
    "morgan": "^1.9.1",
    "nodemailer": "^5.1.1",
    "nodemailer-express-handlebars": "^3.0.0",
    "nodemailer-mock": "^1.3.16",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.8.1",
    "pg-hstore": "^2.3.2",
    "request": "^2.87.0",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "sinon": "^7.2.7",
    "sinon-chai": "^3.3.0",
    "sinon-express-mock": "^2.1.0",
    "slug": "^0.9.1",
    "slugify": "^1.3.4",
    "socket.io": "^2.2.0",
    "swagger-jsdoc": "^3.2.7",
    "underscore": "^1.9.1",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "chai-integer": "^0.1.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^6.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-prepare": "^0.1.0",
    "nodemon": "^1.18.10",
