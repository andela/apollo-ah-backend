{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d dist",
    "test": "mocha",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov || coveralls",
    "start": "nodemon --exec babel-node src/index.js",
    "sequelize": "sequelize",
    "db:create": "sequelize db:create --env",
    "make:migration": "sequelize migration:create --name",
    "make:model": "sequelize model:create --name",
    "migrate": "sequelize db:migrate",
    "migrate:refresh": "npm run migrate:reset && npm run migrate",
    "migrate:reset": "sequelize db:migrate:undo:all",
    "migrate:undo": "npm run sequelize -- db:migrate:undo",
    "seed": "sequelize db:seed:all --seeders-path src/database/seeders",
    "start:dev": "babel-node src/index.js"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "app-root-path": "^2.1.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.5.0",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "mongoose": "^5.4.16",
    "mongoose-unique-validator": "^2.0.2",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.8.1",
    "pg-hstore": "^2.3.2",
    "request": "^2.87.0",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "slug": "^0.9.1",
    "swagger-jsdoc": "^3.2.7",
    "underscore": "^1.9.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^6.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  }
}
